<!doctype html>
<html lang="en" ng-app="BossyUIApp">
<head>
    <title>BossyUI</title>
    <link rel="stylesheet" href="https://storage.googleapis.com/code.getmdl.io/1.0.6/material.blue-deep_purple.min.css" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="/dist/css/bossy.css"/>
    <link rel="stylesheet" href="/css/site.css"/>
</head>
<body>
<div class="home-layout mdl-layout mdl-js-layout mdl-layout--fixed-header">
    <header class="mdl-layout__header mdl-layout__header">
        <div class="mdl-layout__header-row">
            <!-- Title -->
            <span class="mdl-layout-title home-logo"></span>
            <span class="mdl-layout-title home-logo-word"></span>
            <!-- Add spacer, to align navigation to the right -->
            <div class="mdl-layout-spacer"></div>
            <!-- Navigation -->
            <nav class="mdl-navigation">
                <a class="mdl-navigation__link" href="/directives">Directives</a>
                <a class="mdl-navigation__link" href="/styles">Styles</a>
                <a class="mdl-navigation__link" href="/contact">Contact</a>
                <a class="mdl-navigation__link" href="https://github.com/buildcom/BossyUI">GitHub</a>
            </nav>
        </div>
    </header>
    {+content}
    {/content}
    <footer class="mdl-mini-footer">
        <div class="mdl-mini-footer__left-section">
            <ul class="mdl-mini-footer__link-list">
                <li><a href="#">&copy; BossyUI</a></li>
                <li><a href="#">Privacy & Terms</a></li>
            </ul>
        </div>
    </footer>
</div>
<script src="/lib/angular/angular.min.js"></script>
<script src="/lib/ui-router/release/angular-ui-router.min.js"></script>
<script src="/lib/material-design-lite/material.min.js"></script>
<script src="/dist/js/bossy.all.js"></script>
{+scripts}{/scripts}
<script src="/js/app.js"></script>
{LRScript|s}
</body>
</html>




